				};
				step1 {
					clk-name = "CLK_S_D_USB_REF";
					clk-rate = <12000000>;
					type = "clock";
				};
				step2 {
					USB_PHY_COMPDIS = <4>;
					type = "sysconf";
				};
				step3 {
					USB_PHY_SQRX = <3>;
					type = "sysconf";
				};
				step4 {
					USB_PHY_TXFSLS = <3>;
					type = "sysconf";
				};
				step5 {
					USB_PHY_TXPREEMPHASIS = <0>;
					type = "sysconf";
				};
				step6 {
					USB_PHY_TXRISE = <0>;
					type = "sysconf";
				};
				step7 {
					USB_PHY_TXVREF = <8>;
					type = "sysconf";
				};
				step8 {
					USB_PHY_TXHSXV = <3>;
					type = "sysconf";
				};
			};

			power-on-seq{
				step0{
					USB_PHY_SLEEPM = <1>;
					type = "sysconf";
				};
				step1{
					USB_HOST_PWR = <0>;
					type = "sysconf";
				};
				step2{
					USB_PWR_ACK = <0>;
					type = "sysconf-feedback";
				};

			};

			power-off-seq{
				step0{
					USB_PHY_SLEEPM = <0>;
					type = "sysconf";
				};
				step1{
					USB_HOST_PWR = <1>;
					type = "sysconf";
				};
				step2{
					USB_PWR_ACK = <1>;
					type = "sysconf-feedback";
				};
			};
		};

