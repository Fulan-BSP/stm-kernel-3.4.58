#include "stig125-padcfg.dtsi"

/ {

	compatible = "st,stig125";
	#address-cells = <1>;
	#size-cells = <1>;

	cpus {
		cpu@0 {
			compatible = "arm,cortex-a9";
		};
		cpu@1 {
			compatible = "arm,cortex-a9";
		};
	};
	intc: interrupt-controller@fffe1000 {
		compatible = "arm,cortex-a9-gic";
		#interrupt-cells = <3>;
		interrupt-controller;
		reg = <0xfffe1000 0x1000>,
		      <0xfffe0100 0x100>;
	};

	timers {
		#address-cells = <1>;
		#size-cells = <1>;
		#interrupt-cells = <3>;
		interrupt-parent = <&intc>;
		ranges;

		timer@fffe0600 {
			compatible = "arm,cortex-a9-twd-timer";
			reg = <0xfffe0600 0x100>;
			interrupts = <1 13 0x04>;
		};

		timer@fffe0200 {
			compatible = "arm,cortex-a9-global-timer";
			reg = <0xfffe0200 0x100>;
			interrupts = <1 11 0x04>;
		};
	};

	L2: cache-controller {
		compatible = "arm,pl310-cache";
	        reg = <0xfffe2000 0x1000>;
	        arm,data-latency = <3 2 2>;
		arm,tag-latency = <1 1 1>;
		cache-unified;
		cache-level = <2>;
	};

        gpio-controllers {
                #address-cells = <1>;
                #size-cells = <1>;
                gpio-controller;
                ranges;
		/* 0-9: PIO_WEST */
		/* PIO 10 bank west edge */
		PIO0: PIO@febe0000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe0000 0x100>;
			bank-name = "PIO0";
		};
		PIO1: PIO@febe1000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe1000 0x100>;
			bank-name = "PIO1";
		};
		PIO2: PIO@febe2000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe2000 0x100>;
			bank-name = "PIO2";
		};
		PIO3: PIO@febe3000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe3000 0x100>;
			bank-name = "PIO3";
		};
		PIO4: PIO@febe4000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe4000 0x100>;
			bank-name = "PIO4";
		};
		PIO5: PIO@febe5000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe5000 0x100>;
			bank-name = "PIO5";
		};
		PIO6: PIO@febe6000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe6000 0x100>;
			bank-name = "PIO6";
		};
		PIO7: PIO@febe7000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe7000 0x100>;
			bank-name = "PIO7";
		};
		PIO8: PIO@febe8000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe8000 0x100>;
			bank-name = "PIO8";
		};
		PIO9: PIO@febe9000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfebe9000 0x100>;
			bank-name = "PIO9";
		};

		/* 10-18: PIO_SOUTH */
		/* PIO 10 bank on south edge */
		PIO10: PIO@fef70000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef70000 0x100>;
			bank-name = "PIO10";
		};
		PIO11: PIO@fef71000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef71000 0x100>;
			bank-name = "PIO11";
		};
		PIO12: PIO@fef72000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef72000 0x100>;
			bank-name = "PIO12";
		};
		PIO13: PIO@fef73000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef73000 0x100>;
			bank-name = "PIO13";
		};
		PIO14: PIO@fef74000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef74000 0x100>;
			bank-name = "PIO14";
		};
		PIO15: PIO@fef75000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef75000 0x100>;
			bank-name = "PIO15";
		};
		PIO16: PIO@fef76000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef76000 0x100>;
			bank-name = "PIO16";
		};
		PIO17: PIO@fef77000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef77000 0x100>;
			bank-name = "PIO17";
		};
		PIO18: PIO@fef78000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfef78000 0x100>;
			bank-name = "PIO18";
		};

		/* 19-22: PIO_NORTH */
		/* PIO 10 bank on north edge */
		PIO19: PIO@fea00000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea00000 0x100>;
			interrupts      =  <0 20 0>;
			bank-name = "PIO19";
		};
		PIO20: PIO@fea01000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea01000 0x100>;
			interrupts      =  <0 21 0>;
			bank-name = "PIO20";
		};
		PIO21: PIO@fea02000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea02000 0x100>;
			interrupts      =  <0 22 0>;
			bank-name = "PIO21";
		};
		PIO22: PIO@fea03000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea03000 0x100>;
			interrupts      =  <0 23 0>;
			bank-name = "PIO22";
		};

		/* SBC_PIO0-4 */
		/* PIO inside SBC */
		PIO23: PIO@fea60000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea60000 0x100>;
			bank-name = "SBC_PIO0";
		};
		PIO24: PIO@fea61000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea61000 0x100>;
			bank-name = "SBC_PIO1";
		};
		PIO25: PIO@fea62000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea62000 0x100>;
			bank-name = "SBC_PIO2";
		};
		PIO26: PIO@fea63000 {
			#gpio-cells = <1>;
			compatible = "st,gpio";
			reg = <0xfea63000 0x100>;
			bank-name = "SBC_PIO3";
		};
	};

	gpio-irqmux {
		#interrupt-cells = <3>;
		#address-cells = <1>;
		#size-cells = <1>;
		interrupt-parent = <&intc>;
		ranges;
		/*PIO0-10: PIO_WEST */
		gpio-irqmux@febef080{
			compatible      = "st,gpio-irqmux", "simple-bus";
			reg =   <0xfebef080 0x4>;
			interrupts = <0 18 0x0>;
			first-port = <0>;
			ports = <10>;
		};

		/*PIO10-18: PIO_SOUTH */
		gpio-irqmux@fef7f080{
			compatible      = "st,gpio-irqmux", "simple-bus";
			reg =   <0xfef7f080 0x4>;
			interrupts = <0 19 0x0>;
			first-port = <10>;
			ports = <9>;
		};

		/*PIO23-26: PIO_SBC */
		gpio-irqmux@fea6f080{
			compatible      = "st,gpio-irqmux", "simple-bus";
			reg =   <0xfea6f080 0x4>;
			interrupts = <0 24 0x0>;
			first-port = <23>;
			ports = <4>;
		};
	};

	sysconfs {
                #address-cells = <1>;
                #size-cells = <1>;
		ranges;
		compatible	= "simple-bus";

		sysconf@febf0000{
			compatible= "st,sysconf";
			reg = <0xfebf0000 0xe4>;
			group@0 {
				group = <0>;
				offset = <0>;
				start	= <0>;
				group-name = "SYSCFG_WEST";
			};
		};
		sysconf@fefa0000{
			compatible= "st,sysconf";
			reg = <0xfefa0000 0x150>;
			group@0 {
				group = <1>;
				offset = <0>;
				start	= <200>;
				group-name = "SYSCFG_SOUTH";
			};
		};
		sysconf@fea10000{
			compatible= "st,sysconf";
			reg = <0xfea10000 0x7c>;
			group@0 {
				group = <2>;
				offset = <0>;
				start	= <400>;
				group-name = "SYSCFG_NORTH";
			};
		};
		sysconf@fe9a0000{
			compatible= "st,sysconf";
			reg = <0xfe9a0000 0x118>;
			group@0 {
				group = <3>;
				offset = <0>;
				start	= <700>;
				group-name = "SYSCFG_CPU";
			};
		};
		sysconf@fe9b0000{
			compatible= "st,sysconf";
			reg = <0xfe9b0000 0x38>;
			group@0 {
				group = <4>;
				offset = <0>;
				start	= <800>;
				group-name = "SYSCFG_DDR";
			};
		};
		sysconf@fe930000{
			compatible= "st,sysconf";
			reg = <0xfe930000 0xbc>;
			group@0 {
				group = <5>;
				offset = <0>;
				start	= <900>;
				group-name = "SYSCFG_HD";
			};
		};
		sysconf@fea50000{
			compatible= "st,sysconf";
			reg = <0xfea50000 0xb4>;
			group@0 {
				group = <6>;
				offset = <0>;
				start	= <0>;
				group-name = "SYSCFG_SBC";
			};
		};
		sysconf@fecb5100 {
			compatible= "st,sysconf";
			reg = <0xfecb5100 0x54>;
			group@0 {
				group = <7>;
				offset = <0>;
				start	= <0>;
				group-name = "LPM";
			};
		};

	};

	sysconf: sysconf{
		#sysconf-group-cells	= <3>;
		sysconf-groups = <0    56 0>,
				 <200 283 1>,
				 <400 430 2>,
				 <705 769 3>,
				 <800 813 4>,
				 <900 944 5>;

		sysconf-names  = "SYSCFG_WEST",
				 "SYSCFG_SOUTH",
				 "SYSCFG_NORTH",
				 "SYSCFG_CPU",
				 "SYSCFG_DDR",
				 "SYSCFG_HD";
	};

	sbc_sysconf: sbcsysconf{
		#sysconf-group-cells	= <3>;
		sysconf-groups = <0 36 6>;
		sysconf-names  = "SYSCFG_SBC";
	};

	lpm_sysconf: lpmsysconf{
		#sysconf-group-cells	= <3>;
		sysconf-groups = <0 10 7>;
		sysconf-names  = "LPM";
	};

